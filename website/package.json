    "build": "gatsby build"
  },
  "dependencies": {
  },
  "devDependencies": {
    "gatsby": "2.2.0-rc.2",
    "gatsby-plugin-styletron": "^3.0.5",
    "ocular-gatsby": "1.0.0-alpha.17",
    "sharp": "0.22.0"
  }
}
