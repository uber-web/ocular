{
  "description": "A website for Ocular, built with Ocular (Gatsby version)",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "yarn clean && yarn develop",
    "clean": "rm -rf ./.cache ./public",
    "develop": "gatsby develop --port=8001",
    "build": "gatsby build --path-prefix",
    "serve": "gatsby serve --path-prefix",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -d public"
  },
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "devDependencies": {
    "gatsby": "^2.3.0",
    "gatsby-plugin-styletron": "^3.0.5",
    "gh-pages": "^2.0.1",
    "ocular-gatsby": "1.0.0-alpha.25",
    "sharp": "0.22.1"
  }
}
