{
  "name": "ocular-website",
  "version": "0.5.0",
  "description": "A documentation website for Ocular, made with Ocular",
  "main": "index.js",
  "scripts": {
    "start": "ocular start",
    "build": "ocular build-docs && ocular build",
    "lint": "ocular lint",
    "clean": "rm -rf ./dist/* && rm -rf ../docs/*{.js,.css,index.html,appcache,fonts,images}",
    "publish": "npm run clean && npm run build && mv dist/* ../docs"
  },
  "keywords": ["ocular"],
  "author": "jerome@uber.com",
  "license": "MIT",
  "devDependencies": {
    "ocular": "^0.5.0"
  },
  "dependencies": {
    "save-dev": "^2.0.0"
  }
}
