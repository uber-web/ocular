{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "noImplicitAny": false,
    "allowJs": true,
    "checkJs": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "skipLibCheck": true
  },
  "include": [
    "modules",
    "test"
  ],
  "exclude":[
    "node_modules"
  ]
}
