    "tape": "4.11.0",
    "tape-promise": "^4.0.0",
    "typescript": "^4.3.4",
    "webpack": "^4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies_notes": [
    "Note: tape 4.12 and higher no longer compares 0 and -0 equally",
    "Note: prettier >2.13.0 breaks on typescript import type"
  ],
  "dependencies-webpack4-comment": {
